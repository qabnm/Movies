<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <solid android:color="@color/colorFFFFFF"/>
        </shape>
    </item>

    <item android:bottom="33dp">
        <bitmap
            android:gravity="bottom"
            android:filter="true"
            android:scaleType="center"
            android:antialias="true"
            android:src="@drawable/slash_icon2" />

    </item>

    <item android:top="180dp">
        <bitmap
            android:gravity="top"
            android:filter="true"
            android:scaleType="center"
            android:antialias="true"
            android:src="@drawable/slash_icon1" />
    </item>
</layer-list>