<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TopBarLayout">
        <attr name="backSrc" format="reference"/>
        <attr name="contentTitle" format="string"/>
        <attr name="contentTextColor" format="color"/>
        <attr name="contentTextSize" format="dimension"/>
        <attr name="rightVisible" format="enum">
            <enum name="VISIBLE" value="1"/>
            <enum name="GONE" value="2"/>
        </attr>
        <attr name="rightText" format="string"/>
        <attr name="rightTextSize" format="dimension"/>
        <attr name="rightTextColor" format="color"/>

    </declare-styleable>

    <declare-styleable name="EmptyLayout">
        <attr name="imgResource" format="reference"/>
        <attr name="emptyText" format="string"/>
        <attr name="visibility" format="enum">
            <enum name="VISIBLE" value="1"/>
            <enum name="GONE" value="2"/>
        </attr>
        <attr name="emptyTextSize" format="dimension"/>
        <attr name="emptyTextColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="RoundCornerLayout">
        <attr name="radius" format="dimension"/>
        <attr name="solidColor" format="color"/>
    </declare-styleable>
</resources>